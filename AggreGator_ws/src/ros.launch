<?xml version="1.0"?>
<!--xml file for roslaunch
	roslaunch will navigate through xml file launching all nodes automatically
-->

<launch>
	<!--Hardware Interface nodes-->
	<node pkg="hardware_interface" name="adafruit_node" type="adafruit_node.cpp">
	<node pkg="hardware_interface" name="adc_node" type="adc_node.cpp">
	<node pkg="hardware_interface" name="i2c_node" type="i2c_node.cpp">
	<node pkg="hardware_interface" name="ina226_node" type="ina266_node.cpp">
	
	<!--Joy nodes-->
	<node pkg="joy" name="joy" type="joy_node.cpp">
	<!--Joy parameters-->
	
	<!--LADAR Nodes-->
	<node pkg="urg" name="urg_node" type="?">
	<node pkg="ladar" name="ladar_node" type="ladar_node.cpp">
	
	<!--Motor Controller nodes-->
	<node pkg="motor_controller" name="bucket_motor_node" type="bucket_motor_node.cpp">
	<node pkg="motor_controller" name="linear_actuator_node" type="linear_actuator_node.cpp">
	<node pkg="motor_controller" name="talker" type="talker.cpp">
	<node pkg="motor_controller" name="wheel_motor_node" type="wheel_motor_node.cpp">
	
	<!--Power Monitering nodes-->
	<node pkg="power_monitoring" name="power_monitoring_node" type="power_monitoring_node.cpp">
	
	<!--Remote Control nodes-->
	<node pkg="remote_control" name="remote_control_node" type="xbox_controller_node.cpp">
	<!--Remote Control Paramaters-->
	<param name="remote_control/send_freq" value="10">
	<param name="remote_control/xbox_controller/mapping" value="2">
	
	
</launch>

